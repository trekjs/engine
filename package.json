{
  "name": "trek-engine",
  "version": "1.0.4",
  "description": "Micro, Elegant, Fast, Lightweight, Expressive middleware for Modern Node.js",
  "repository": "trekjs/engine",
  "author": {
    "name": "Fangdun Cai",
    "email": "cfddream@gmail.com",
    "url": "fundon.me"
  },
  "license": "MIT",
  "keywords": [
    "trek",
    "web",
    "app",
    "http",
    "application",
    "framework",
    "middleware",
    "modern"
  ],
  "main": "lib/index.js",
  "scripts": {
    "lint": "xo",
    "precommit": "lint-staged",
    "test": "xo && nyc ava"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "accepts": "1.3.4",
    "content-disposition": "0.5.2",
    "content-type": "1.0.2",
    "cookies": "0.7.1",
    "fresh": "0.5.0",
    "mime-types": "2.1.16",
    "on-finished": "2.3.0",
    "parseurl": "1.3.1",
    "range-parser": "1.2.0",
    "trek-http-error": "0.0.5",
    "trek-middleware": "1.2.0",
    "type-is": "1.6.15",
    "vary": "1.1.1"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "eslint-config-prettier": "2.3.0",
    "husky": "0.14.3",
    "lint-staged": "4.0.4",
    "nyc": "11.1.0",
    "prettier": "1.6.1",
    "request": "2.81.0",
    "request-promise": "4.2.1",
    "xo": "0.18.2"
  },
  "ava": {
    "concurrency": 5,
    "failFast": true,
    "files": [
      "test/**/*.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "xo",
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "nyc": {
    "reporter": [
      "html",
      "lcov",
      "text"
    ]
  },
  "xo": {
    "extends": "prettier"
  }
}
